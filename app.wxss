/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.tip {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  color: #999;
  padding: 0 34rpx;
  padding-top: 36rpx;
  box-sizing: border-box;
}

.tip image {
  width: 200rpx;
  height: 248rpx;
}
.arrow-r{
  width: 14rpx;
  height: 26rpx;
}